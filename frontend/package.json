{
  "name": "meeshy-frontend",
  "version": "1.0.41-alpha",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3100",
    "prebuild": "pnpm run copy:shared && rm -rf .next && rm shared/schema.prisma 2> /dev/null || true",
    "copy:shared": "echo 'Shared files already copied in Docker context'",
    "build": "next build",
    "start": "next start -p 3100 -H 0.0.0.0",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "./scripts/docker-build-optimized.sh",
    "docker:build:fast": "PUSH=false PLATFORMS=linux/arm64 ./scripts/docker-build-optimized.sh",
    "docker:build:separate": "./scripts/docker-build-separate.sh",
    "docker:build:local": "PUSH=false ./scripts/docker-build-optimized.sh",
    "docker:build:legacy": "docker buildx build --platform linux/arm64,linux/amd64 -t isopen/meeshy-frontend:latest -f ./Dockerfile .",
    "docker:push": "pnpm run docker:build && docker push isopen/meeshy-frontend:latest"
  },
  "dependencies": {
    "@meeshy/shared": "workspace:*",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "@vercel/og": "^0.8.5",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "pdfjs-dist": "^5.4.394",
    "react": "^19.0.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.5.3",
    "react-pdf": "^10.2.0",
    "recharts": "^2.15.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tinyld": "^1.3.4",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "postcss": "^8.5.6",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
