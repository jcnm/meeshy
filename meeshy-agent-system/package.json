{
  "name": "meeshy-agent-system",
  "version": "1.0.0",
  "description": "Intelligent conversational agents for Meeshy platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/cli.ts",
    "start": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'",
    "agent": "tsx src/cli.ts"
  },
  "keywords": [
    "meeshy",
    "agent",
    "conversation",
    "ai",
    "chatbot"
  ],
  "author": "Meeshy Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "better-sqlite3": "^9.4.3",
    "chromadb": "^1.8.1",
    "compromise": "^14.11.0",
    "dotenv": "^16.4.5",
    "natural": "^6.10.2",
    "openai": "^4.28.4",
    "p-queue": "^8.0.1",
    "sentiment": "^5.0.2",
    "yaml": "^2.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
