{
  "name": "meeshy-fastify-service",
  "version": "1.0.0",
  "description": "Fastify WebSocket service for Meeshy chat application",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run prebuild && tsc",
    "prebuild": "cd ../shared && npm run generate",
    "start": "node dist/server.js",
    "dev": "npm run prebuild && tsx watch src/server.ts",
    "generate": "cd ../shared && prisma generate",
    "migrate": "cd ../shared && prisma migrate dev",
    "seed": "tsx src/scripts/seed.ts",
    "test": "jest --config=jest.config.json",
    "test:watch": "jest --config=jest.config.json --watch",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "proto:generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin ../shared/proto/messaging.proto"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/websocket": "^10.0.1",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@prisma/client": "6.13.0",
    "bcryptjs": "^3.0.2",
    "fastify": "^4.26.2",
    "jsonwebtoken": "^9.0.2",
    "nice-grpc": "^2.1.12",
    "pino-pretty": "^11.0.0",
    "prisma": "^6.11.1",
    "protobufjs": "^7.5.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zeromq": "^6.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.11.30",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "grpc-tools": "^1.12.4",
    "jest": "^30.0.5",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
