{
  "name": "meeshy-translator",
  "version": "1.0.0",
  "description": "FastAPI Translation service for Meeshy multi-language messaging",
  "scripts": {
    "dev": "python main.py",
    "start": "python main.py",
    "test": "python -m pytest",
    "test:watch": "python -m pytest --watch",
    "install": "pip install -r requirements.txt",
    "generate": "cd ../shared && prisma generate --generator=client-py"
  },
  "keywords": [
    "fastapi",
    "translation",
    "ml",
    "multilingual",
    "grpc"
  ],
  "author": "Meeshy Team",
  "private": true,
  "main": "index.js",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "prisma": "^6.13.0"
  },
  "dependencies": {
    "@prisma/client": "6.13.0"
  }
}
